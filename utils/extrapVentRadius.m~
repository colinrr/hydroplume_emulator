function r = extrapVentRadius(Q)
% Conduit radius extrapolation for control scenarios. Based on an emprical
% curve fit 
%   Q = MER (kg/s)
% Q = 10^9.7;

%     dDir     = '~/Kahuna/data/glaciovolc/coupledSweeps/manuscript_v1_submitted_data/';
%     codeDir  = '~/code/research-projects/glaciovolc/glaciovolc-dev/';
% 
%     datFile = 'outputSummary_coupledSweep_2021-09-02_MWIv2_hiLat_n1785_35q_0zw.mat';
% 
%     load(fullfile(dDir,datFile))

    % Emprical power law curve fit for radius as f of q
%     rfun = @(q) 6.059e-06.*log10(q).^7.559 + 5.073;
%     rfun = @(q) 6.084e-06.*log10(q).^7.556 + 5.072;
    
    % Empirical curve fit for conduit area
    C1 = 0.009913;
    C2 = 0.7267;
    C3 = 58.04;
    afun = @(x) C1.*(x).^C2 + C3;

%     r = rfun(Q);
    r = (afun(Q)./pi).^(1/2);
    
end